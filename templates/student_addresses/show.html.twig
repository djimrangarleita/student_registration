{% extends 'base.html.twig' %}

{% block title %}StudentAddresses{% endblock %}

{% block body %}
    <h1>StudentAddresses</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ student_address.id }}</td>
            </tr>
            <tr>
                <th>Date_address_from</th>
                <td>{{ student_address.dateAddressFrom ? student_address.dateAddressFrom|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_address_to</th>
                <td>{{ student_address.dateAddressTo ? student_address.dateAddressTo|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Monthly_rental</th>
                <td>{{ student_address.monthlyRental }}</td>
            </tr>
            <tr>
                <th>Other_details</th>
                <td>{{ student_address.otherDetails }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('student_addresses_index') }}">back to list</a>

    <a href="{{ path('student_addresses_edit', {'id': student_address.id}) }}">edit</a>

    {{ include('student_addresses/_delete_form.html.twig') }}
{% endblock %}
