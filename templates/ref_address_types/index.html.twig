{% extends 'base.html.twig' %}

{% block title %}RefAddressTypes index
{% endblock %}

{% block body %}
	<div class="container">
		<h1>RefAddressTypes index</h1>

		<table class="table">
			<thead>
				<tr>
					<th>Id</th>
					<th>Address_type_code</th>
					<th>Address_type_description</th>
					<th>actions</th>
				</tr>
			</thead>
			<tbody>
				{% for ref_address_type in ref_address_types %}
					<tr>
						<td>{{ ref_address_type.id }}</td>
						<td>{{ ref_address_type.addressTypeCode }}</td>
						<td>{{ ref_address_type.addressTypeDescription }}</td>
						<td>
							<a href="{{ path('ref_address_types_show', {'id': ref_address_type.id}) }}">show</a>
							<a href="{{ path('ref_address_types_edit', {'id': ref_address_type.id}) }}">edit</a>
						</td>
					</tr>
				{% else %}
					<tr>
						<td colspan="4">no records found</td>
					</tr>
				{% endfor %}
			</tbody>
		</table>

		<a href="{{ path('ref_address_types_new') }}">Create new</a>
	</div>
{% endblock %}
