{{ form_start(form) }}
<div class="row">
	<div class="col-12 col-md-2 col-lg-4">
		{{ form_row(form.line_1_number_building) }}
	</div>
	<div class="col-12 col-md-2 col-lg-4">
		{{ form_row(form.line_2_number_street) }}
	</div>
	<div class="col-12 col-md-2 col-lg-4">
		{{ form_row(form.line_3_area_locality) }}
	</div>
	<div class="col-12 col-md-2 col-lg-4">
		{{ form_row(form.city) }}
	</div>
	<div class="col-12 col-md-2 col-lg-4">
		{{ form_row(form.zip_code) }}
	</div>
	<div class="col-12 col-md-2 col-lg-4">
		{{ form_row(form.state_province_country) }}
	</div>
	<div class="col-12 col-md-2 col-lg-4">
		{{ form_row(form.country) }}
	</div>
	<div class="col-12 col-md-2 col-lg-4">
		{{ form_row(form.landlord) }}
	</div>
	<div class="col-12 col-md-2 col-lg-4">
		{{ form_row(form.university_accommodation_yn) }}
	</div>
	<div class="col-12">
		{{ form_row(form.other_address_details) }}
	</div>
</div>
<button class="btn btn-outline-success">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
